{
  "agent": {
    "inline_assistant_model": {
      "provider": "copilot_chat",
      "model": "gpt-4o"
    },
    "default_model": {
      "provider": "zed.dev",
      "model": "claude-3-5-sonnet-20241022"
    },
    "button": true,
    "dock": "right",
    "default_width": 400
  },
  "context_servers": {
    "heroui": {
      "command": "npx",
      "args": ["-y", "@heroui/mcp"],
      "env": {}
    },
    "heroui-react": {
      "command": "npx",
      "args": ["-y", "@heroui/react-mcp@latest"],
      "env": {}
    },
    "supabase-mcp": {
      "url": "https://mcp.supabase.com/mcp?project_ref=${SUPABASE_PROJECT_REF}",
      "headers": {
        "Authorization": "Bearer ${SUPABASE_ACCESS_TOKEN}"
      }
    }
  },
  "languages": {
    "TypeScript": {
      "format_on_save": {
        "external": {
          "command": "prettier",
          "arguments": ["--stdin-filepath", "{buffer_path}"]
        }
      },
      "language_servers": [
        "typescript-language-server",
        "tailwindcss-language-server"
      ],
      "tab_size": 2,
      "hard_tabs": false
    },
    "TSX": {
      "format_on_save": {
        "external": {
          "command": "prettier",
          "arguments": ["--stdin-filepath", "{buffer_path}"]
        }
      },
      "language_servers": [
        "typescript-language-server",
        "tailwindcss-language-server"
      ],
      "tab_size": 2,
      "hard_tabs": false
    },
    "JavaScript": {
      "format_on_save": {
        "external": {
          "command": "prettier",
          "arguments": ["--stdin-filepath", "{buffer_path}"]
        }
      },
      "language_servers": [
        "typescript-language-server",
        "tailwindcss-language-server"
      ],
      "tab_size": 2,
      "hard_tabs": false
    },
    "CSS": {
      "format_on_save": {
        "external": {
          "command": "prettier",
          "arguments": ["--stdin-filepath", "{buffer_path}"]
        }
      },
      "language_servers": [
        "tailwindcss-language-server",
        "css-language-server"
      ],
      "tab_size": 2,
      "hard_tabs": false
    },
    "JSON": {
      "format_on_save": {
        "external": {
          "command": "prettier",
          "arguments": ["--stdin-filepath", "{buffer_path}"]
        }
      },
      "tab_size": 2,
      "hard_tabs": false
    }
  },
  "lsp": {
    "typescript-language-server": {
      "initialization_options": {
        "preferences": {
          "includeInlayParameterNameHints": "all",
          "includeInlayParameterNameHintsWhenArgumentMatchesName": true,
          "includeInlayFunctionParameterTypeHints": true,
          "includeInlayVariableTypeHints": false,
          "includeInlayPropertyDeclarationTypeHints": true,
          "includeInlayFunctionLikeReturnTypeHints": true
        }
      }
    },
    "tailwindcss-language-server": {
      "settings": {
        "tailwindCSS": {
          "experimental": {
            "classRegex": [
              ["cn\\(([^)]*)\\)", "[\"'`]([^\"'`]*)[\"'`]"],
              ["cva\\(([^)]*)\\)", "[\"'`]([^\"'`]*)[\"'`]"],
              ["className\\s*=\\s*[\"'`]([^\"'`]*)[\"'`]"],
              ["class\\s*=\\s*[\"'`]([^\"'`]*)[\"'`]"]
            ]
          },
          "includeLanguages": {
            "typescript": "javascript",
            "typescriptreact": "javascript"
          }
        }
      }
    }
  },
  "project_panel": {
    "button": true,
    "dock": "left",
    "default_width": 240
  },
  "outline_panel": {
    "button": true,
    "dock": "right",
    "default_width": 240
  },
  "collaboration_panel": {
    "button": false
  },
  "autosave": {
    "after_delay": {
      "milliseconds": 1000
    }
  },
  "terminal": {
    "button": true,
    "dock": "bottom",
    "default_height": 320,
    "working_directory": "current_project_directory",
    "shell": {
      "program": "pwsh"
    }
  },
  "tabs": {
    "close_position": "right",
    "file_icons": true,
    "git_status": true
  },
  "scrollbar": {
    "show": "auto",
    "cursors": false,
    "git_diff": true,
    "search_results": true,
    "selected_symbol": true,
    "diagnostics": "error"
  },
  "inlay_hints": {
    "enabled": true,
    "show_type_hints": true,
    "show_parameter_hints": true,
    "show_other_hints": true
  },
  "git": {
    "git_gutter": "tracked_files",
    "inline_blame": {
      "enabled": true,
      "delay_ms": 1000
    }
  },
  "vim_mode": false,
  "cursor_blink": false,
  "seed_search_query_from_cursor": "always",
  "ui_font_size": 14,
  "buffer_font_size": 14,
  "buffer_font_family": ".ZedMono",
  "theme": {
    "mode": "system",
    "light": "One Light",
    "dark": "Ayu Dark"
  },
  "diagnostics": {
    "include_warnings": true
  },
  "file_scan_exclusions": [
    "**/.git",
    "**/node_modules",
    "**/.next",
    "**/dist",
    "**/build",
    "**/.supabase"
  ],
  "auto_update": true,
  "format_on_save": "on"
}
